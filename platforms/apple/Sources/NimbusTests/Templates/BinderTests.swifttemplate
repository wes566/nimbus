        XCTAssertEqual(result, .some(<% if index > 2 { %><%= arities.takeAsSumOfBinaryCallback(position: .second, count: index - 1)%><% } else { %><%= arities.takeAsSumOfBinaryCallback(position: .second, count: index + 1) %><% } %>))
